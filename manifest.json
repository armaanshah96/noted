{
   "author": "Armaan",
   "browser_action": {
      "default_popup": "popup.html#noted",
      "default_icon": "public/icons/128.png"
   },
   "content_scripts": [ {
      "js": [
         "assets/scripts/noted.js"
      ],
      "css": ["css/content.css"],
      "matches": [ "\u003Call_urls>" ]
   } ],
   "description": "Mark notable ideas and thoughts with minimal interruption when you're reading",
   "homepage_url": "https://github.com/armaanshah96/noted",
   "icons": {
      "128": "public/icons/128.png",
      "16": "public/icons/16.png",
      "48": "public/icons/48.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "Noted",
   "permissions": [ "activeTab", "storage" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.0",
   "web_accessible_resources": [ "public/icons/*.png" ]
}
